{
  "name": "angularj-crud",
  "version": "0.0.0",
  "description": "CRUD example AngularJS",
  "authors": [
    "Ermal Aliraj"
  ],
  "license": "MIT",
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000"
  },
  "dependencies": {
    "ng-table": "^3.0.1"
  },
  "devDependencies": {},
  "paths": {
    "html": "./app/**/*.html",
    "htmltemplates": "./app/**/*.html",
    "vendorjs": [
      "./bower_components/jquery/dist/jquery.min.js",
      "./bower_components/angular/angular.min.js",
      "./bower_components/angular-animate/angular-animate.min.js",
      "./bower_components/angular-route/angular-route.min.js",
      "./bower_components/angular-sanitize/angular-sanitize.min.js",
      "./bower_components/bootstrap/dist/js/bootstrap.min.js",
      "./bower_components/toastr/toastr.min.js",
      "./bower_components/moment/min/moment.min.js",
      "./bower_components/extras.angular.plus/ngplus-overlay.js",
      "./bower_components/breezejs/breeze.debug.js",
      "./bower_components/breezejs/adapters/breeze.dataService.mongo.js",
      "./bower_components/breezejs/labs/breeze.angular.js",
      "./bower_components/breezejs/labs/breeze.directives.js",
      "./bower_components/breezejs/labs/breeze.metadata-helper.js",
      "./bower_components/breezejs/labs/breeze.saveErrorExtensions.js",
      "./bower_components/angular-ui-bootstrap-bower/ui-bootstrap-tpls.min.js",
      "./bower_components/angular.breeze.storagewip/angular.breeze.storagewip.js"
    ],
    "vendorcss": [
      "./bower_components/bootstrap/dist/css/bootstrap.min.css",
      "./bower_components/font-awesome/css/font-awesome.min.css",
      "./bower_components/toastr/toastr.css",
      "./bower_components/breezejs/labs/breeze.directives.css"
    ],
    "css": [
      "./app/content/customtheme.css",
      "./app/content/styles.css"
    ],
    "js": [
      "./app/**/*module*.js",
      "./app/**/*.js",
      "!./app/**/{,/*-spaghetti.js}"
    ],
    "fonts": [
      "./bower_components/bootstrap/fonts/**/*.*",
      "./bower_components/font-awesome/fonts/**/*.*"
    ],
    "images": [
      "./app/content/images/**/*"
    ],
    "stage": "./build/stage/",
    "build": "./build/"
  }
}
